{
  "name": "convoypanel/panel",
  "type": "project",
  "description": "A beautifully designed KVM server management panel for Proxmox Virtual Environment.",
  "keywords": [
    "proxmox",
    "kvm",
    "virtualization",
    "hosting",
    "SaaS",
    "server",
    "panel"
  ],
  "license": "BSL",
  "require": {
    "php": "^8.2",
    "ext-gmp": "*",
    "ext-pcntl": "*",
    "caseyamcl/guzzle_retry_middleware": "^2.8",
    "guzzlehttp/guzzle": "7.4.5",
    "laravel/fortify": "^1.16",
    "laravel/framework": "^10.33",
    "laravel/helpers": "^1.6",
    "laravel/horizon": "^5.12",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "lcobucci/jwt": "^5.0",
    "phpseclib/phpseclib": "^3.0",
    "spatie/eloquent-sortable": "^4.0",
    "spatie/laravel-data": "^2.2",
    "spatie/laravel-fractal": "^6.0",
    "spatie/laravel-query-builder": "^5.1",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.64",
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.2",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "pestphp/pest-plugin-laravel": "^2.2",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "files": [
      "app/Helpers/Network.php"
    ],
    "psr-4": {
      "Convoy\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "analyze": [
      "./vendor/bin/phpstan analyse --memory-limit=4G"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
