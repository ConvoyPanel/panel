{
    "name": "convoypanel/panel",
    "type": "project",
    "description": "A beautifully designed KVM server management panel for Proxmox Virtual Environment.",
    "keywords": [
        "proxmox",
        "kvm",
        "virtualization",
        "hosting",
        "SaaS",
        "server",
        "panel"
    ],
    "license": "BSL",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "ext-pcntl": "*",
        "caseyamcl/guzzle_retry_middleware": "^2.8",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/fortify": "^1.20",
        "laravel/framework": "^11.27",
        "laravel/helpers": "^1.7",
        "laravel/horizon": "^5.22",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "lcobucci/jwt": "^5.0",
        "phpseclib/phpseclib": "^3.0",
        "spatie/eloquent-sortable": "^4.2",
        "spatie/laravel-data": "^4.1",
        "spatie/laravel-fractal": "^6.1",
        "spatie/laravel-query-builder": "^5.8",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.64",
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "files": [
            "app/Helpers/Network.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyze": [
            "./vendor/bin/phpstan analyse --memory-limit=4G"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
